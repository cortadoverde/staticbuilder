<section class="set-up-block">
  <div class="container">
    <div class="set-up-box">
      <div class="row">
        {% for boxitem in section.data.items %}
        <div class="col-md-6">
          <i class="{{boxitem.icon}}"></i>
          <h4>{{boxitem.title}}</h4>
          <p>{{boxitem.description}}</p>
        </div>
        {% endfor %}
      </div>
    </div>
  </div>
</section>